{
  "buildCommand": "bash build.sh",
  "outputDirectory": "build/web",
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.+)",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
